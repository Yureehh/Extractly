  "CAI": {
    "description": "Friendly Traffic Accident Report: form to declare the details of a road accident between two vehicles.",
    "fields": [
      {
        "name": "Accident Date",
        "description": "Date of the accident"
      },
      {
        "name": "Accident Time",
        "description": "Time when the accident occurred"
      },
      {
        "name": "Driver A",
